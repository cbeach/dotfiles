{
  "segments": [
    "~/.bash/powerline-shell-custom-nonsense/hostname.py",
    "virtual_env",
    "aws_profile",
    "ssh",
    "cwd",
    "git",
    "git_stash",
    "jobs",
    "set_term_title",
    "newline",
    "~/.bash/powerline-shell-custom-nonsense/time.py",
    "~/.bash/powerline-shell-custom-nonsense/custom_root.py"
  ],
  "hostname": {
    "colorize": true
  },
  "cwd": {
    "mode": "plain",
    "max_depth": 6
  },
  "time": {
    "format": "%H:%M",
    "time_zones": ["US/Pacific", "UTC"]
  },
  "theme": "~/.bash/powerline-shell-custom-nonsense/custom_theme.py"
}
